{
  "name": "avenirs-cofolio-client",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "clean:dist": "rimraf dist",
    "build-only": "vite build",
    "build-only:development": "vite build -m development",
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "build:development": "run-p type-check \"build-only:development {@}\" --",
    "format": "eslint . --fix",
    "lint": "eslint .",
    "preview": "vite preview",
    "preview:development": "vite preview -m development",
    "test": "npm run test:unit",
    "test:coverage": "vitest run --coverage",
    "test:unit": "vitest run",
    "test:a11y": "run-s -c test:a11y:run test:a11y:cleanup",
    "test:a11y:run": "playwright test --config=a11y/playwright.a11y.config.ts",
    "test:a11y:cleanup": "rimraf a11y/.output",
    "test:watch": "vitest",
    "type-check": "vue-tsc --build --force",
    "prepare": "husky || true && npm run generate:api",
    "commitlint": "commitlint --edit",
    "lint-staged": "lint-staged",
    "generate:api": "orval",
    "postgenerate:api": "eslint . --fix ./src/api"
  },
  "dependencies": {
    "@gouvfr/dsfr": "^1.13.2",
    "@gouvminint/vue-dsfr": "^8.4.0",
    "@tanstack/vue-query": "^5.74.9",
    "axios": "^1.9.0",
    "date-fns": "^4.1.0",
    "focus-trap": "^7.5.4",
    "focus-trap-vue": "^4.0.3",
    "lodash-es": "^4.17.21",
    "pinia": "^3.0.3",
    "pinia-plugin-persistedstate": "^4.3.0",
    "vue": "^3.5.13",
    "vue-i18n": "^11.1.3",
    "vue-router": "^4.4.3"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.0.0",
    "@axe-core/playwright": "^4.10.1",
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@playwright/test": "^1.52.0",
    "@rushstack/eslint-patch": "^1.10.4",
    "@tsconfig/node20": "^20.1.4",
    "@tsconfig/node22": "^22.0.1",
    "@types/jsdom": "^21.1.7",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.16.2",
    "@vitejs/plugin-vue": "^5.1.3",
    "@vitejs/plugin-vue-jsx": "^4.0.1",
    "@vitest/coverage-v8": "^3.1.3",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.5.1",
    "axe-html-reporter": "^2.2.11",
    "eslint": "^9.9.1",
    "eslint-plugin-playwright": "^2.2.0",
    "husky": "^9.1.7",
    "jsdom": "^25.0.0",
    "lint-staged": "^15.5.2",
    "msw": "^2.10.2",
    "npm-run-all": "^4.1.5",
    "npm-run-all2": "^6.2.2",
    "orval": "^7.9.0",
    "rimraf": "^5.0.10",
    "sass-embedded": "^1.87.0",
    "typescript": "~5.4.5",
    "unplugin-auto-import": "^0.18.2",
    "unplugin-vue-components": "^0.27.4",
    "vite": "^6.3.3",
    "vite-plugin-vue-devtools": "^7.3.9",
    "vitest": "^3.1.3",
    "vue-tsc": "^2.1.2",
    "workbox-window": "^7.1.0"
  },
  "lint-staged": {
    "*": "eslint --fix"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  }
}
